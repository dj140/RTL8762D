<?xml version="1.0"?>
<configuration>
	<system.diagnostics>
		<trace autoflush="true" indentsize="2">
			<listeners> 
				<remove name="Default"/>
				<add name="myListener" type="System.Diagnostics.TextWriterTraceListener" initializeData="DebugAnalyzer.log" /> 
			</listeners> 
		</trace> 
	</system.diagnostics> 
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
	</startup>
</configuration>
